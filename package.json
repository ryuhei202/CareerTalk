{
  "name": "career-talk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3003",
    "build": "next build",
    "start": "next start -p 3003",
    "test": "vitest run",
    "clean": "rimraf .next/cache/fetch-cache",
    "clean-dev": "run-s clean dev",
    "clean-build": "run-s clean build",
    "clean-start": "run-s clean build start",
    "db-migrate-deploy": "prisma migrate deploy",
    "migrate": "prisma migrate dev && prisma generate",
    "db-push": "prisma db push",
    "db-seed": "prisma migrate reset",
    "db-studio": "prisma studio",
    "postinstall": "prisma generate",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "run-s format:*",
    "format:eslint": "eslint --fix './src/**/*.{js,ts,tsx}'",
    "format:prettier": "prettier --write './src/**/*.{js,ts,tsx}'"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.20.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "install": "^0.13.0",
    "inversify": "^6.0.2",
    "lodash": "^4.17.21",
    "lucide": "^0.452.0",
    "lucide-react": "^0.452.0",
    "next": "14.2.14",
    "next-auth": "^4.24.8",
    "npm-run-all": "^4.1.5",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.5.3",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.1",
    "@types/lodash": "^4.17.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.14",
    "jsdom": "^25.0.1",
    "postcss": "^8",
    "prisma": "^5.20.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^2.1.3"
  },
  "type": "module",
  "volta": {
    "node": "20.18.0",
    "npm": "10.8.2"
  }
}
